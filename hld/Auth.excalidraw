      "type": "text",
      "x": 319,
      "y": 1308.5714285714284,
      "width": 1234.9591064453125,
      "height": 200,
      "angle": 0,
      "strokeColor": "#1971c2",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
