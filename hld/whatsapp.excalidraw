    {
      "id": "0QSv152tiLdKxulu4vDPj",
      "type": "text",
      "x": 623.7204396081353,
      "y": 4228.809523809524,
      "width": 1242.319091796875,
      "height": 250,
      "angle": 0,
      "strokeColor": "#1971c2",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
